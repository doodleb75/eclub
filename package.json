{
  "name": "eclub",
  "version": "1.0.0",
  "description": "eclub project with sass and live-server",
  "main": "pc/index.html",
  "scripts": {
    "sass": "sass assets/sass:assets/css --no-source-map",
    "watch": "sass --watch assets/sass:assets/css --no-source-map",
    "serve": "live-server --port=8081 --host=127.0.0.1",
    "dev": "concurrently \"npm run watch\" \"npm run serve\""
